<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.proyectosController">
   <children>
      <Button layoutX="24.0" layoutY="360.0" mnemonicParsing="false" onAction="#volver" prefHeight="25.0" prefWidth="58.0" text="Volver" />
      <TableView fx:id="tablaProyectos" layoutY="-4.0" prefHeight="354.0" prefWidth="421.0">
         <columns>
            <TableColumn fx:id="idCol" prefWidth="37.0" text="ID" />
            <TableColumn fx:id="paisCol" prefWidth="202.0" text="País" />
         </columns>
      </TableView>
      <Button fx:id="listProyectosBtn" layoutX="315.0" layoutY="358.0" mnemonicParsing="false" onAction="#verProyectos" prefHeight="25.0" prefWidth="106.0" text="Ver proyectos" />
      <Label layoutX="436.0" layoutY="14.0" prefWidth="47.0" text="Pais" />
      <TextField fx:id="paisProyectoTf" layoutX="437.0" layoutY="31.0" />
      <Button fx:id="addProyectoBtn" layoutX="437.0" layoutY="234.0" mnemonicParsing="false" onAction="#addProyecto" prefHeight="25.0" prefWidth="149.0" text="Añadir Proyecto" />
      <Button fx:id="searchProyectoBtn" layoutX="436.0" layoutY="273.0" mnemonicParsing="false" onAction="#searchProyecto" prefHeight="25.0" prefWidth="149.0" text="Buscar Proyecto" />
      <Button fx:id="delTProyectoBtn" layoutX="437.0" layoutY="311.0" mnemonicParsing="false" onAction="#delProyecto" prefHeight="25.0" prefWidth="149.0" text="Eliminar Proyecto" />
      <Button fx:id="updProyectoBtn" layoutX="97.0" layoutY="358.0" mnemonicParsing="false" onAction="#updateProyecto" prefHeight="25.0" prefWidth="149.0" text="Actualizar proyectos" />
      <Label layoutX="437.0" layoutY="337.0" text="Id:" />
      <TextField fx:id="idProyectoTf" layoutX="436.0" layoutY="354.0" prefHeight="25.0" prefWidth="47.0" />
   </children>
</AnchorPane>
